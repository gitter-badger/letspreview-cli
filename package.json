{
  "name": "letspreview-cli",
  "version": "0.0.0-semantically-released.0",
  "description": "A commandline tool for Lets PReview",
  "main": "letspreview.js",
  "engines": {
    "node": ">=4.0"
  },
  "bin": {
    "letspreview": "letspreview.js"
  },
  "scripts": {
    "validate": "npm run lint && npm run test",
    "test": "jasmine ./spec/**/* --verbose",
    "lint": "eslint *.js spec/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salte-io/letspreview-cli.git"
  },
  "author": "The Salte Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "chalk": "^1.1.3",
    "form-data": "^2.1.2",
    "node-fetch": "^1.6.3",
    "tar.gz": "^1.0.5",
    "uuid": "^3.0.1",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "jasmine": "^2.5.3",
    "pact": "^2.3.1"
  }
}
